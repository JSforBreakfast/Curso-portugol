algoritmo "conte ate 10"
var
   contador, valor: inteiro
inicio
   contador <- 10
   escreval("Quer contar ate quanto? ")
   leia(valor)
   enquanto (contador <= valor) faca
   escreval(contador)
   contador <- contador + 1
   fimenquanto
   escreval("Terminei a contagem.")
fimalgoritmo
